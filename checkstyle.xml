<?xml version="1.0"?>
<!DOCTYPE module PUBLIC
        "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN"
        "https://checkstyle.org/dtds/configuration_1_3.dtd">

<module name="Checker">
    <!-- Checks for violations of the Google Java Style Guide -->
    <module name="com.puppycrawl.tools.checkstyle.TreeWalker">
        <!-- Enforces the Google Java Style -->
        <module name="com.puppycrawl.tools.checkstyle.checks.GoogleJavaFormatCheck">
            <!-- Path to the google java format configuration -->
            <property name="config" value="google_checks.xml"/>
        </module>
    </module>
</module>
